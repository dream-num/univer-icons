{
    "name": "@univerjs/icons",
    "version": "0.3.24",
    "description": "React icons for Univer.",
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "homepage": "https://univer.ai/icons",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dream-num/univer-icons"
    },
    "bugs": {
        "url": "https://github.com/dream-num/univer-icons/issues"
    },
    "keywords": [
        "icons"
    ],
    "files": [
        "dist",
        "esm"
    ],
    "main": "dist/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "scripts": {
        "build": "pnpm build:esm && pnpm build:cjs",
        "build:cjs": "ncc build src/index.ts -o dist -m -e react",
        "build:esm": "tsc --module ES6 --outDir esm"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@vercel/ncc": "^0.38.3",
        "camelcase": "^6.2.0",
        "gulp": "^4.0.2",
        "gulp-filter": "^7.0.0",
        "gulp-rename": "^2.0.0",
        "typescript": "^5.8.3"
    }
}