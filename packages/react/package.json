{
  "name": "@univerjs/icons",
  "version": "0.0.10",
  "description": "React icons used by Univer.",
  "license": "MIT",
  "files": [
        "dist/",
        "esm/"
  ],
  "main": "dist/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "scripts": {
      "build": "yarn build:esm && yarn build:cjs",
      "build:cjs": "ncc build src/index.ts -o dist -m -e react",
      "build:esm": "tsc --module ES6 --outDir esm"
  },
  "peerDependencies": {
      "react": "*",
      "react-dom": "*"
  },
  "devDependencies": {
      "@types/react": "^17.0.0",
      "@vercel/ncc": "^0.27.0",
      "camelcase": "^7.0.1",
      "cross-env": "^7.0.3",
      "gulp": "^4.0.2",
      "gulp-filter": "^7.0.0",
      "gulp-rename": "^2.0.0",
      "typescript": "^4.1.3"
  }
}
